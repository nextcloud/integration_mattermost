		"stylelint:fix": "stylelint src/**/*.vue src/**/*.scss src/**/*.css --fix"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nextcloud/integration_slack"
	},
	"keywords": [
		"slack"
	],
